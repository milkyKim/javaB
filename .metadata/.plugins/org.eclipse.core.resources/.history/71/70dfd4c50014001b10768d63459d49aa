package ch13;

// 스레드 클래스 작성
class MyThread extends Thread{

	@Override
	public void run() {
		int n=0;
		for(int i=0; i<10; i++) {
			System.out.println("숫자: "+i);
		}
	}
	
}

public class TestThread {

	public static void main(String[] args) {
		// 스레드 객체 생성
		MyThread mt = new MyThread();
	
		// 스레드 시작
		mt.start();
	}

}
